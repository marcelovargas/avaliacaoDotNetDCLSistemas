@model Avaliacao.Data.Models.Call

@{
    ViewData["Title"] = "Mostrar cadastro de ligação";
}

<h1>@ViewData["Title"]</h1>


<div>
   
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Begin)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Begin)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.End)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.End)
        </dd>
    </dl>
</div>

<div class="form-group">
    <a asp-action="Edit" class="btn btn-outline-warning" asp-route-id="@Model.Id">Edit</a>
    <a asp-action="Index" class="btn btn-outline-success"><i class="fa fa-chevron-left"></i> Volver</a>
</div>
